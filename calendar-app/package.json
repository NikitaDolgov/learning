{
  "name": "calendar-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.3.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.1",
    "serve": "^5.2.4"
  },
  "scripts": {
    "now-start": "cd build && serve -s ./",
    "deploy": "now -e REACT_APP_SECRET_CODE=secretcode",
    "deploy-hard": "rm -rf package-lock.json && rm -rf node-modules && npm i && now -e REACT_APP_SECRET_CODE=secretcode",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
